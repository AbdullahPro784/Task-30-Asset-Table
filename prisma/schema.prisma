generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Item {
  id          String   @id @default(cuid())
  serial      String
  category    String
  brand       String
  type        String
  vehicle     String
  statusState String
  statusLevel Int?
  endDate     String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
